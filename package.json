{
  "name": "fast-react-particles",
  "version": "1.0.1",
  "description": "A fast and basic version of particlesjs",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/*.js",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "clear": "rimraf -f ./dist",
    "start": "webpack --watch --mode development",
    "build": "yarn clear && webpack --mode production && yarn bundlesize",
    "bundlesize": "gzip -c dist/index.js | wc -c",
    "pub": "yarn build && np --no-tests"
  },
  "keywords": [
    "particlesjs",
    "canvas",
    "animation",
    "particles"
  ],
  "author": "Luca Campli",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "np": "^6.3.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "unused-files-webpack-plugin": "^3.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
